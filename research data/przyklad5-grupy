<script language="javascript">


function SubmitRemovingExchange(classId, grNo)
{
    var div;
    div = document.getElementById('exchange_remove');
    div.innerHTML = "<form action='kontroler.php' class='greenforms' method='post' enctype='multipart/form-data'>" +
        "<input type='hidden' name='_action' value='actionx:dla_stud/rejestracja/brdg2/usunGielde()' />" +
        "<input type='hidden' name='csrftoken' value='2015-09-30-00cd230c0429410d' />" +
        "<input type='hidden' name='rej_kod' value='0851-2015/16Z' />" +
        "<input type='hidden' name='prz_kod' value='0800-31KKP-DW' />" +
        "<input type='hidden' name='cdyd_kod' value='2015/16Z' />" +
        "<input type='hidden' name='zajecia[" + classId + "][]' value='" + grNo + "' />" +
        "<input type='hidden' name='callback' value='g_afbbf28c' />" +
        "<input type='submit' class='submit' id='exchange_remove_submit'/></form>";
    document.getElementById('exchange_remove_submit').click();
}

function IsAnyGroupChecked()
{
    var inputs = document.getElementsByTagName('input');
    for(var i = 0; i < inputs.length; i++) {
    
            if((inputs[i].className == 'group_action_180760') && inputs[i].checked)
            return true;
        
    }
    alert('Nie wybrałeś żadnej grupy');
    return false;
}


