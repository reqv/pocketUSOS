<script type='text/javascript'>
    $(function() {
        $('.countdown').on('countdowncomplete', function() {
            $('.semitransparent').removeClass('semitransparent');
        });

        $('.ajaxForm').ajaxForm({
            beforeSubmit: function(form) {
                form.find('.submit').val('W trakcie...');
            },
            complete: function(form) {
                form.find('.submit').val('Rejestruj');
            }
        });

        $('.ajaxForm').on('success', function() {
            $('input').attr('disabled', 'disabled');
        });
    });
</script>

